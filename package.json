{
  "name": "no-more-wordpress",
  "version": "1.0.0",
  "description": "No More WordPress allows you to 1) convert WordPress DB to static files 2) generate static websites based on 1 3) eliminate PHP and MySQL",
  "main": "index.js",
  "scripts": {
    "convert": "node bin/convert.mjs",
    "build": "node bin/build.mjs --mode=prod",
    "build:dev": "node bin/build.mjs --mode=dev",
    "start": "node bin/start.mjs --mode=prod",
    "start:dev": "node bin/start.mjs --mode=dev",
    "draft": "node bin/draft.mjs",
    "publish": "node bin/publish.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otchy210/no-more-wordpress.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/otchy210/no-more-wordpress/issues"
  },
  "homepage": "https://github.com/otchy210/no-more-wordpress#readme",
  "devDependencies": {
    "csso": "^4.2.0",
    "datauri": "^3.0.0",
    "express": "^4.17.1",
    "html-minifier": "^4.0.0",
    "jsdom": "^16.4.0",
    "mysql": "^2.18.1",
    "recursive-copy": "^2.0.11",
    "uglify-js": "^3.12.5"
  }
}
